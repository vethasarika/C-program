#include <stdio.h>

int main() {
    int n, i, age;
    int youngest = 1000, oldest = -1;
    int invalid = 0;

    
    scanf("%d", &n);

    if (n <= 0) {
        printf("Invalid Input\n");
        return 0;
    }

    for (i = 0; i < n; i++) {
        scanf("%d", &age);

        if (age < 0) {
            invalid = 1;
            break;
        }

        if (age < youngest)
            youngest = age;

        if (age > oldest)
            oldest = age;
    }

    if (invalid) {
        printf("Invalid Input\n");
    } else {
        printf("Youngest=%d\n", youngest);
        printf("Oldest=%d\n", oldest);
    }

    return 0;
}
