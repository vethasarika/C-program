#include <stdio.h>

#include <ctype.h>  

int isVowel(char ch) {

    ch = tolower(ch);  

    return (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u');

}

int main() {

    char str[1000];

    int count = 0, i = 0;

   

    fgets(str, sizeof(str), stdin);

    

    while (str[i] != '\0') {

        if (isVowel(str[i])) {

            count++;

        }

        i++;

    }

   

    printf("%d\n", count);

    return 0;

}
