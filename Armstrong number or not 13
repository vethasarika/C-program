#include <stdio.h>

#include <math.h>

int main() {

    int num, original, remainder, result = 0, digits = 0;



    scanf("%d", &num);



    original = num;

    

    int temp = num;

    while (temp != 0) {

        temp /= 10;

        digits++;

    }



    temp = num;

    while (temp != 0) {

        remainder = temp % 10;

        result += pow(remainder, digits);

        temp /= 10;

    }

    

    if (result == original)

        printf("Yes\n");

    else

        printf("No\n");

    return 0;

}
