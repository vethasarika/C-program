#include <stdio.h>

#include <math.h>

int main() {

    float floatVal;

    int squareInput, base, exponent;

   

    scanf("%f", &floatVal);

    scanf("%d", &squareInput);

    scanf("%d", &base);

    scanf("%d", &exponent);

   

    printf("%d\n", (int)floor(floatVal));    

    printf("%d\n", (int)ceil(floatVal));    

    printf("%d\n", (int)sqrt(squareInput)); 

    printf("%d\n", (int)pow(base, exponent));

    return 0;

}
