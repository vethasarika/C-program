#include <stdio.h>

int main() {

    int a, b, c;

    

    scanf("%d %d %d", &a, &b, &c);

   

    if (a > b) {

        if (a > c) {

            printf("a is largest then b and c\n");

        } else {

            printf("c is largest then a and b\n");

        }

    } else {

        if (b > c) {

            printf("b is largest then a and c\n");

        } else {

            printf("c is largest then a and b\n");

        }

    }

    return 0;

}





    

