#include <stdio.h>

int main() {

    int n, num;

    int pos = 0, neg = 0;

    scanf("%d", &n);  

    for (int i = 0; i < n; i++) {

        scanf("%d", &num);

        if (num > 0) {

            pos++;

        } else if (num < 0) {

            neg++;

        }

    }

   

    printf("Count of Positive Integer is %.2f\n", (float)pos);

    printf("Count of Negative Integer is %.2f\n", (float)neg);

    return 0;

}







  

